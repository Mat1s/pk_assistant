<div class="container-fluid">
  <div class='row'>
    <div class = 'col-md-6 titles text-center'>  
      <h3><%= link_to t('layouts.header.title'), root_url %></h3>   
      <p><%= t('layouts.header.phrase') %></p>
      <p><%= link_to '<<<', 'javascript:history.back()' %></p>
    </div>
    <div class='col-md-6'>
      <div class='row'>
        <div class='social col-md-7'>
          <%= render 'layouts/link_for_enter' %>
        </div>
        <div class='lang col-md-5'>
          <%= link_to t('layouts.header.search'), search_path %><br />
          <%= render 'layouts/lang' %>
        </div>
      </div>
    </div>
  </div>
</div>